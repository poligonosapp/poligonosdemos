{
  "npmScope": "nx-poligonosapp",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "poligonosapp-nx-cloud-demo": { "tags": [] },
    "poligonosapp-nx-cloud-demo-e2e": {
      "tags": [],
      "implicitDependencies": ["poligonosapp-nx-cloud-demo"]
    }
  }
}
