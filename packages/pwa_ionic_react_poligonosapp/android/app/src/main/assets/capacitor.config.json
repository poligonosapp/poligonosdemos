{
  "appId": "com.github.luismendes070.poligonosdemos.poligonosapp",
  "appName": "poligonosapp",
  "bundledWebRuntime": true,
  "npmClient": "npm",
  "webDir": "build",
  "linuxAndroidStudioPath": "/home/luism/Downloads/android-studio/bin/studio.sh",
  "compress": true,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    }
  },
  "cordova": {},
  "windowsAndroidStudioPath": "C:\\Program Files\\Android\\Android Studio1\\bin\\studio64.exe",
  "hideLogs": false,
  "overrideUserAgent": "my custom user agent",
  "appendUserAgent": "string to append",
  "backgroundColor": "#ffffff",
  "android": {
    "overrideUserAgent": "my custom user agent for Android",
    "appendUserAgent": "string to append for Android",
    "backgroundColor": "#ffffff",
    "allowMixedContent": "true",
    "captureInput": true,
    "webContentsDebuggingEnabled": true,
    "hideLogs": true
  },
  "ios": {
    "overrideUserAgent": "my custom user agent for iOS",
    "appendUserAgent": "string to append for iOS",
    "backgroundColor": "#ffffff",
    "contentInset": "always",
    "cordovaSwiftVersion": "4.2",
    "minVersion": "11.3",
    "cordovaLinkerFlags": [
      "-ObjC"
    ],
    "allowsLinkPreview": false,
    "hideLogs": true
  },
  "server": {
    "url": "https://www.poligonosapp.herokuapp.com:8100",
    "cleartext": true
  }
}
