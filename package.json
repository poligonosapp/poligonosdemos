{
  "name": "poligonosdemos",
  "version": "1.1.1",
  "type": "module",
  "private": true,
  "packages": [
    "pwa_ionic_react\\poligonosApp",
    "app"
  ],
  "projects": [
    "poligonosapp",
    "app",
    "ui",
    "server",
    "docker-nginx",
    "frontend",
    "backend"
