{
    "version": "1.4.6",
    "useWorkspaces": true,
    "private": true,
    "dependencies": {
        "react": "17.0.1"
    },
    "devDependencies": {
        "lerna": "^2.2.0",
        "react": "^16.8.0 || ^16.11.0 || ^17.0.1",
        "quicklink": "^2.0.0 || ^2.1.0",
        "yarn": "2.x"
    },
    "name": "poligonosdemos",
    "npmClient": "yarn",
    "command": {
        "publish": {
            "message": "chore(release): publish",
            "registry": "https://npm.pkg.github.com"
        },
        "bootstrap": {
            "npmClientArgs": [
                "--production",
                "--no-optional",
                "--no-package-lock"
            ]
        }
    },
    "projects": [
        "@poligonosdemos/pwa_ionic_react_poligonosapp",
        "@poligonosdemos/app",
        "@poligonosdemos/packages",
        "@poligonosdemos/poligonos",
        "@poligonosdemos/ui",
        "@poligonosdemos/server",
        "@poligonosdemos/docker-nginx",
        "@poligonosdemos/frontend",
        "@poligonosdemos/backend",
        "@poligonosdemos/geojson_jackson_rn_demo",
        "@poligonosdemos/nx-poligonosapp",
        "@poligonosdemos/nextjs_poligonos_poligono"
    ],
    "packages": [
        "pwa_ionic_react_poligonosapp",
        "pwa_ionic_react_poligonosapp/tests",
        "packages/nx-poligonosapp",
        "packages/progressive-web-app",
        "app",
        "nextjs_poligonos",
        "nextjs_poligonos_poligono"
    ],
    "workspaces": [
        "@poligonosdemos/package.json",
        "@poligonosdemos/pwa_ionic_react_poligonosapp/package.json",
        "@poligonosdemos/ycppoligono",
        "@poligonosdemos/nextjs_poligonos/poligono",
        "@poligonosdemos/packages/*",
        "@poligonosdemos/packages/progressive-web-app",
        "@poligonosdemos/app/*",
        "@poligonosdemos/njs/ui/*",
        "@poligonosdemos/server/*",
        "@poligonosdemos/ui/*",
        "@poligonosdemos/ycppoligono/*",
        "@poligonosdemos/poligonos/*",
        "@poligonosdemos/ronin-ui/*",
        "@poligonosdemos/z-app-name/*",
        "@poligonosdemos/progressive-web-app/*",
        "@poligonosdemos/ui-next-js-heroku/*",
        "@poligonosdemos/poligonosapp/*",
        "@poligonosdemos/poligono_angular_cli_one/*",
        "@poligonosdemos/angular_cli_one_poligono/*",
        "@poligonosdemos/nextjs_poligonos_poligono/*"
    ]
}
